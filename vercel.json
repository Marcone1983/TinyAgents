{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "public": true,
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "api/telegram.py"
    },
    {
      "src": "/api/stripe-webhook",
      "dest": "api/stripe_webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
