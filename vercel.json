{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "api/telegram.py"
    },
    {
      "src": "/api/stripe-webhook",
      "dest": "api/stripe_webhook.py"
    },
    {
      "src": "/main.js",
      "dest": "main.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}
